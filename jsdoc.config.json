{
  "source": {
    "include": ["src"],
    "includePattern": "\\.(ts|tsx|js|jsx)$",
    "excludePattern": "(\\.test\\.|\\.spec\\.|\\.stories\\.|(\\\\|/)stories(\\\\|/))"
  },
  "plugins": ["jsdoc-babel"],
  "babel": {
    "extensions": ["ts", "tsx", "js", "jsx"]
  },
  "opts": {
    "destination": "static/jsdoc",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "encoding": "utf8"
  }
}
